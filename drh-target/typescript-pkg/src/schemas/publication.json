{  
  "type": "object",
  "properties": {
    "publication_id": {
      "type": [
        "string",
        "null"
      ],
      "pattern": "^[A-Z0-9][A-Z0-9-]{1,12}[A-Z0-9]$"
    },
    "publication_title": {
      "type": [
        "string",
        "null"
      ]
    },
    "digital_object_identifier": {
      "type": [
        "string",
        "null"
      ],
      "pattern": "^10\\.\\d{4,9}/[-._;()/:a-zA-Z0-9]+$"
    },
    "publication_site": {
      "type": [
        "string",
        "null"
      ]
    },
    "study_id": {
      "type": [
        "string",
        "null"
      ],
      "$ref": "study.json#/properties/study_id",
      "pattern": "^[A-Z0-9]{4,6}$"
    },
    "tenant_id": {
      "type": [
        "string",
        "null"
      ]
    },
    "tenant_name": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "publication_id",
    "publication_title",
    "digital_object_identifier",
    "publication_site",
    "study_id",
    "tenant_id",
    "tenant_name"
  ]
}