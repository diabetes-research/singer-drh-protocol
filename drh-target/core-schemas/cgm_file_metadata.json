{  
  "type": "object",
  "properties": {
    "metadata_id": {
      "type": [
        "string",
        "null"
      ],
      "pattern": "^[A-Z0-9][A-Z0-9-]{1,12}[A-Z0-9]$"
    },
    "devicename": {
      "type": [
        "string",
        "null"
      ],
      "enum": [
        "Freestyle Libre",
        "Clarity",
        "Carelink",
        "Tidepool",
        "Dexcom G4",
        "Dexcom G5",
        "Dexcom G6",
        "Dexcom G7",
        "Stelo",
        "FreeStyle Libre 2",
        "FreeStyle Libre 3",
        "FreeStyle Libre Pro",
        "Guardian Connect",
        "Simplera CGM System",
        "Eversense",
        "Eversense XL",
        "Eversense E3",
        "Eversense 365",
        "FreeStyle Navigator",
        "Medtronic Paradigm",
        null
      ]
    },
    "device_id": {
      "type": [
        "string",
        "null"
      ]
    },
    "source_platform": {
      "type": [
        "string",
        "null"
      ],
      "enum": [
        "Abbott",
        "Dexcom",
        "Medtronic",
        "Tidepool",
        "Senseonics",
        null
      ]
    },
    "patient_id": {
      "type": [
        "string",
        "null"
      ],
      "$ref": "participant.json#/properties/participant_id",
      "pattern": "^[A-Z0-9][A-Z0-9-]{1,12}[A-Z0-9]$"
    },
    "file_name": {
      "type": [
        "string",
        "null"
      ]
    },
    "file_format": {
      "type": [
        "string",
        "null"
      ]
    },
    "file_upload_date": {
      "type": [
        "string",
        "null"
      ],
      "format": "date",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "data_start_date": {
      "type": [
        "string",
        "null"
      ],
      "format": "date",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "data_end_date": {
      "type": [
        "string",
        "null"
      ],
      "format": "date",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "map_field_of_cgm_date": {
      "type": [
        "string",
        "null"
      ]
    },
    "map_field_of_cgm_value": {
      "type": [
        "string",
        "null"
      ]
    },
    "study_id": {
      "type": [
        "string",
        "null"
      ],
      "$ref": "study.json#/properties/study_id",
      "pattern": "^[A-Z0-9]{4,6}$"
    },
    "map_field_of_patient_id": {
      "type": [
        "string",
        "null"
      ]
    },
    "tenant_id": {
      "type": [
        "string",
        "null"
      ]
    },
    "tenant_name": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "metadata_id",
    "devicename",
    "source_platform",
    "patient_id",
    "file_name",
    "file_format",
    "map_field_of_cgm_date",
    "map_field_of_cgm_value",
    "study_id",
    "map_field_of_patient_id",
    "tenant_id",
    "tenant_name"
  ]
}