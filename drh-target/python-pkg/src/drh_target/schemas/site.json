{  
  "type": "object",
  "properties": {
    "study_id": {
      "type": [
        "string",
        "null"
      ],
      "$ref": "study.json#/properties/study_id",
      "pattern": "^[A-Z0-9]{4,6}$"
    },
    "site_id": {
      "type": [
        "string",
        "null"
      ],
      "pattern": "^[A-Z0-9][A-Z0-9-]{1,12}[A-Z0-9]$"
    },
    "site_name": {
      "type": [
        "string",
        "null"
      ]
    },
    "site_type": {
      "type": [
        "string",
        "null"
      ]
    },
    "tenant_id": {
      "type": [
        "string",
        "null"
      ]
    },
    "tenant_name": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "study_id",
    "site_id",
    "site_name",
    "site_type",
    "tenant_id",
    "tenant_name"
  ]
}